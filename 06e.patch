-//! Session-based pseudo-mapper
-class Basket extends Magic {
-
-	//@{ Error messages
-	const
-		E_Field='Undefined field %s';
-	//@}
-
-	protected
-		//! Session key
-		$key,
-		//! Current item identifier
-		$id,
-		//! Current item contents
-		$item=array();
-
-	/**
-	*	Return TRUE if field is defined
-	*	@return bool
-	*	@param $key string
