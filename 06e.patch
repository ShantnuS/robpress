+//! Image manipulation tools
+class Image {
+
+	//@{ Messages
+	const
+		E_Color='Invalid color specified: %s',
+		E_File='File not found',
+		E_Font='CAPTCHA font not found',
+		E_TTF='No TrueType support in GD module',
+		E_Length='Invalid CAPTCHA length: %s';
