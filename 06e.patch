+			$error && in_array($error['type'],
+			[E_ERROR,E_PARSE,E_CORE_ERROR,E_COMPILE_ERROR]))
+			// Fatal error detected
+			$this->error(500,
+				sprintf(self::E_Fatal,$error['message']),[$error]);
+	}
