+		while (ob_get_level())
+			$out=ob_get_clean().$out;
+		header('Content-Encoding: none');
+		header('Content-Length: '.strlen($out));
+		header('Connection: close');
+		session_commit();
+		echo $out;
+		flush();
+		if (function_exists('fastcgi_finish_request'))
+			fastcgi_finish_request();
+	}
