+		FLAG_False=0,
+		FLAG_True=1,
+		FLAG_Both=2;
+	//@}
+
+	protected
+		//! Test results
+		$data=[],
+		//! Success indicator
+		$passed=TRUE;
+
+	/**
+	*	Return test results
+	*	@return array
+	**/
