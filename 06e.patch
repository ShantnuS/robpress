-				for ($i=0;$i<$raw;$i++)
-					$iv.=chr(mt_rand(0,255));
-			$salt=str_replace('+','.',base64_encode($iv));
-		}
-		$salt=substr($salt,0,$len);
-		$hash=crypt($pw,sprintf('$2y$%02d$',$cost).$salt);
-		return strlen($hash)>13?$hash:FALSE;
-	}
